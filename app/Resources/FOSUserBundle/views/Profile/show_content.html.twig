{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show">
    <div class="register">
        <div class="container">
            <p>{{ 'profile.show.lastName'|trans }}: {{ user.lastName }}</p>
            <p>{{ 'profile.show.firstName'|trans }}: {{ user.firstName }}</p>
            <p>{{ 'profile.show.username'|trans }}: {{ user.username }}</p>
            <p>{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
            <p>{{ 'profile.show.billing'|trans }}: {{ user.billing }}</p>
            <p>{{ 'profile.show.delivery'|trans }}: {{ user.delivery }}</p>
            <a href="{{ path('fos_user_profile_edit') }}">{{ 'profile.edit.title'|trans }}</a>
        </div>
    </div>
</div>
