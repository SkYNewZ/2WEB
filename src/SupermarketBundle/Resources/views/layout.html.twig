{% extends 'SupermarketBundle::base.html.twig' %}

{% block breadcrumbs %}
    <!-- breadcrumbs -->
    <div class="breadcrumbs">
        <div class="container">
            <ol class="breadcrumb breadcrumb1 animated wow slideInLeft" data-wow-delay=".5s">
                <li><a href="{{ path('homepage') }}"><span class="glyphicon glyphicon-home"
                                               aria-hidden="true"></span>{{ 'Home'|trans }}</a></li>
                {% block breadcrumbs_title %}{% endblock %}
            </ol>
        </div>
    </div>
    <!-- //breadcrumbs -->
{% endblock %}